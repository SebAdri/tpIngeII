FOREIGN KEY (id_hijo)
REFERENCES public.Hijos (id_hijo)
ON DELETE NO ACTION
ON UPDATE NO ACTION
NOT DEFERRABLE;

ALTER TABLE public.Libreta_Detalle ADD CONSTRAINT libretas_libreta_detalle_fk
FOREIGN KEY (id_lib)
REFERENCES public.Libretas (id_lib)
ON DELETE NO ACTION
ON UPDATE NO ACTION
NOT DEFERRABLE;

ALTER TABLE public.roles ADD CONSTRAINT users_roles_fk
FOREIGN KEY (pk_id)
